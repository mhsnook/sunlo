{
	"productName": "sunlo-tanstack",
	"version": "0.1.0",
	"identifier": "app.sunlo.dev",
	"build": {
		"frontendDist": "../dist",
		"devUrl": "http://localhost:5173",
		"beforeDevCommand": "pnpm dev",
		"beforeBuildCommand": "pnpm build"
	},
	"app": {
		"windows": [
			{
				"title": "Sunlo: Social Language Learning",
				"width": 800,
				"height": 600,
				"resizable": true,
				"fullscreen": false
			}
		],
		"security": {
			"csp": null
		}
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/Square310x310Logo.png",
			"icons/icon.icns",
			"icons/icon.ico"
		],
		"createUpdaterArtifacts": true
	},
	"$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
	"plugins": {
		"updater": {
			"pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IERFNkNDMEMwQjAyM0U5QzIKUldUQzZTT3d3TUJzM3V3Z3FXdjlBMkF1YVJ1TnFndnVmRTluYUllbWNqUGNybGFlc3dGd3VGZVIK",
			"endpoints": [
				"https://cdn.crabnebula.app/update/sunlo/sunlo-tanstack/{{target}}-{{arch}}/{{current_version}}"
			]
		}
	}
}
